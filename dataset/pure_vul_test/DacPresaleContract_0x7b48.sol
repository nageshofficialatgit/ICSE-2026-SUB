// File: contracts/5_DacPresaleContract.sol


pragma solidity ^0.8.21;






contract DacPresaleContract is Ownable, ReentrancyGuard {

    using Math for uint256;

    bool public presaleActive;
    address public usdtAddress;
    AggregatorV3Interface public priceFeed;
    mapping(address => uint256) public contributions;
    mapping(uint256 => uint256) public tierPrices;
    uint256 public tokenSaleLimit;
    uint256 public tokensSold;

    address[] public contributors;
    
    event TokensPurchased(address indexed buyer, uint256 amount, uint256 value, string currency);
    
    constructor(address _initialOwner, address _usdt, address _priceFeed) Ownable(_initialOwner) {
        usdtAddress = _usdt;
        priceFeed = AggregatorV3Interface(_priceFeed);
        
        tierPrices[50000] = 0.08 * 10**6;
        tierPrices[100000] = 0.075 * 10**6;
        tierPrices[200000] = 0.075 * 10**6;
        tierPrices[300000] = 0.075 * 10**6;
        tierPrices[400000] = 0.075 * 10**6;
        tierPrices[500000] = 0.07 * 10**6;
        tierPrices[600000] = 0.07 * 10**6;
        tierPrices[700000] = 0.07 * 10**6;
        tierPrices[800000] = 0.07 * 10**6;
        tierPrices[900000] = 0.07 * 10**6;
        tierPrices[1000000] = 0.065 * 10**6;
        tierPrices[5000000] = 0.06 * 10**6;

        presaleActive = true;
        tokenSaleLimit = 50000000 * 10**18;
        tokensSold = 0;
    }

    function getLatestETHPrice() public view returns (uint256) {
        (, int price, , , ) = priceFeed.latestRoundData();
        return uint256(price) * 10**10; // Convert to 18 decimals
    }

    function getETHPriceForTier(uint256 _tokenAmount) public view returns(uint256) {
        uint256 ethPrice = getLatestETHPrice();
        uint256 _usdtAmount = _tokenAmount * tierPrices[_tokenAmount] * 10 ** 12;
        return (_usdtAmount * 10**18) /ethPrice;
    }

    function getContribution(address _contributor) public view returns (uint256) {
        return contributions[_contributor];
    }

    function buyWithETH(uint256 _tokenAmount) external payable nonReentrant {
        require(presaleActive, "Presale not active");
        uint256 _tokenAmountWithDecimals = _tokenAmount * 10**18;
        require(_tokenAmount > 0, "Send a valid token amount");
        require(tierPrices[_tokenAmount] > 0, "Send a valid token amount");
        require(_tokenAmountWithDecimals + tokensSold <= tokenSaleLimit, "Phase limit reached");
        require(msg.value > 0, "Send ETH to buy tokens");

        uint256 _ethAmount = getETHPriceForTier(_tokenAmount);

        require(_ethAmount == msg.value, "ETH Price Changed");
        
        tokensSold += _tokenAmountWithDecimals;
        contributions[msg.sender] += _tokenAmountWithDecimals;
        contributors.push(msg.sender);
        emit TokensPurchased(msg.sender, _tokenAmountWithDecimals, msg.value, "ETH");
    }

    function buyWithUSDT(uint256 _tokenAmount) external nonReentrant {
        require(presaleActive, "Presale not active");
        uint256 _tokenAmountWithDecimals = _tokenAmount * 10**18;
        require(_tokenAmount > 0, "Send a valid token amount");
        require(tierPrices[_tokenAmount] > 0, "Send a valid token amount");
        require(_tokenAmountWithDecimals + tokensSold <= tokenSaleLimit, "Phase limit reached");

        uint256 _usdtAmount = _tokenAmount * tierPrices[_tokenAmount];
        require(IERC20(usdtAddress).allowance(msg.sender, address(this)) >= _usdtAmount, "Invalid allowance");
        
        IERC20(usdtAddress).transferFrom(msg.sender, address(this), _usdtAmount);
        tokensSold += _tokenAmountWithDecimals;
        contributions[msg.sender] += _tokenAmountWithDecimals;
        contributors.push(msg.sender);
        emit TokensPurchased(msg.sender, _tokenAmountWithDecimals, _usdtAmount, "USDT");
    }

    function togglePresale() external onlyOwner {
        presaleActive = !presaleActive;
    }
    
    function withdrawFunds() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }
    
    function withdrawUSDT() external onlyOwner {
        IERC20(usdtAddress).transfer(owner(), IERC20(usdtAddress).balanceOf(address(this)));
    }

    function addExternalContributor(address _contributor, uint256 _tokenAmount) external onlyOwner {
        require(_tokenAmount * 10**18 + tokensSold <= tokenSaleLimit, "Phase limit reached");
        require(tierPrices[_tokenAmount] > 0, "Send a valid token amount");
        uint256 _usdtAmount = _tokenAmount * tierPrices[_tokenAmount];
        contributions[_contributor] += _tokenAmount * 10**18;
        tokensSold += _tokenAmount * 10**18;
        contributors.push(_contributor);
        emit TokensPurchased(msg.sender, _tokenAmount * 10**18, _usdtAmount, "EXT");
    }

    function getContributorsCount() public view returns(uint256) {
        return contributors.length;
    }
}