// File: Presale.sol


pragma solidity ^0.8.24;




contract DevTokenPresale is Ownable, Pausable {
    // Use SafeMath from DevToken without importing it separately
    using SafeMath for uint256;

    DevToken public token; // Reference to the DevToken contract
    uint256 public rate; // Tokens per ETH (e.g., 1000 tokens per 1 ETH)
    uint256 public startTime; // Presale start timestamp
    uint256 public endTime; // Presale end timestamp
    uint256 public tokensForSale; // Total tokens allocated for presale
    uint256 public tokensSold; // Tokens sold so far
    uint256 public maxEthPerAddress; // Maximum ETH contribution per address
    bool public presaleEnded; // Flag to mark presale as ended

    mapping(address => uint256) public contributions; // Tracks ETH contributed per address

    event TokensPurchased(address buyer, uint256 ethAmount, uint256 tokenAmount);
    event PresaleEnded(uint256 tokensSold, uint256 ethCollected);
    event RateUpdated(uint256 newRate);
    event MaxEthPerAddressUpdated(uint256 newMaxEth);

    constructor(
        address _token,
        uint256 _rate,
        uint256 _startTime,
        uint256 _endTime,
        uint256 _tokensForSale,
        uint256 _maxEthPerAddress
    ) Ownable(msg.sender) {
        require(_token != address(0), "Invalid token address");
        require(_rate > 0, "Rate must be greater than 0");
        require(_startTime >= block.timestamp, "Start time must be in the future");
        require(_endTime > _startTime, "End time must be after start time");
        require(_tokensForSale > 0, "Tokens for sale must be greater than 0");
        require(_maxEthPerAddress > 0, "Max ETH per address must be greater than 0");

        token = DevToken(_token);
        rate = _rate;
        startTime = _startTime;
        endTime = _endTime;
        tokensForSale = _tokensForSale;
        maxEthPerAddress = _maxEthPerAddress;
        tokensSold = 0;
        presaleEnded = false;
    }

    // Fallback function to receive ETH
    receive() external payable whenNotPaused {
        buyTokens();
    }

    // Function for users to buy tokens with ETH
    function buyTokens() public payable whenNotPaused {
        require(block.timestamp >= startTime, "Presale has not started yet");
        require(block.timestamp <= endTime, "Presale has ended");
        require(msg.value > 0, "ETH amount must be greater than 0");
        require(!presaleEnded, "Presale has been manually ended");

        uint256 ethAmount = msg.value;
        uint256 newContribution = contributions[msg.sender].add(ethAmount);
        require(newContribution <= maxEthPerAddress, "Exceeds max ETH per address");

        uint256 tokenAmount = ethAmount.mul(rate); // Calculate token amount based on rate
        require(tokensSold.add(tokenAmount) <= tokensForSale, "Not enough tokens left for sale");

        // Update state
        contributions[msg.sender] = newContribution;
        tokensSold = tokensSold.add(tokenAmount);

        // Transfer tokens to buyer
        require(token.transfer(msg.sender, tokenAmount), "Token transfer failed");

        emit TokensPurchased(msg.sender, ethAmount, tokenAmount);
    }

    // Owner can end the presale manually
    function endPresale() external onlyOwner {
        require(!presaleEnded, "Presale already ended");
        presaleEnded = true;
        emit PresaleEnded(tokensSold, address(this).balance);
    }

    // Owner can withdraw collected ETH at any time
    function withdrawETH(uint256 amount) external onlyOwner {
        uint256 ethBalance = address(this).balance;
        require(ethBalance > 0, "No ETH to withdraw");
        require(amount <= ethBalance, "Amount exceeds balance");

        (bool sent, ) = msg.sender.call{value: amount}("");
        require(sent, "Failed to send ETH");
    }

    // Owner can withdraw remaining tokens at any time
    function withdrawTokens(uint256 amount) external onlyOwner {
        uint256 remainingTokens = token.balanceOf(address(this));
        require(remainingTokens > 0, "No tokens to withdraw");
        require(amount <= remainingTokens, "Amount exceeds available tokens");

        require(token.transfer(msg.sender, amount), "Token transfer failed");
    }

    // Owner can update the token rate during the presale
    function setRate(uint256 newRate) external onlyOwner {
        require(newRate > 0, "Rate must be greater than 0");
        rate = newRate;
        emit RateUpdated(newRate);
    }

    // Owner can update the max ETH per address
    function setMaxEthPerAddress(uint256 newMaxEth) external onlyOwner {
        require(newMaxEth > 0, "Max ETH must be greater than 0");
        maxEthPerAddress = newMaxEth;
        emit MaxEthPerAddressUpdated(newMaxEth);
    }

    // Owner can pause the contract (stops buying)
    function pause() external onlyOwner {
        _pause();
    }

    // Owner can unpause the contract
    function unpause() external onlyOwner {
        _unpause();
    }

    // View function to check remaining tokens for sale
    function getRemainingTokens() public view returns (uint256) {
        return tokensForSale.sub(tokensSold);
    }

    // View function to check contract's ETH balance
    function getETHBalance() public view returns (uint256) {
        return address(this).balance;
    }

    // View function to check a user's contribution
    function getContribution(address user) public view returns (uint256) {
        return contributions[user];
    }
}