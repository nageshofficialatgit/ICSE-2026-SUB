// File: NFTBigpartner.sol

pragma solidity ^0.8.9;

contract NFT_BIG_BONUS is ERC1155, Ownable, ERC1155Burnable {
    IERC20 public BIGP;
    address public creator;

    constructor() ERC1155("") {
       creator = msg.sender;
    }

    function setBIPG(IERC20 _BigPointAddr)public returns(bool){
          require(msg.sender==creator);
          BIGP = IERC20(_BigPointAddr);
          return true;
    }

    function setURI(string memory newuri) public onlyOwner {
        _setURI(newuri);
    }

    function mint(
        address account,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public {
        require(msg.sender == rightperson);
        _mint(account, id, amount, data);
    }

    

    uint256 public Batch_id = 0;
    uint256 public BigPAmount;

    mapping(uint256 => uint256) public ReleasedTime;
    mapping(uint256 => address) public MintAddr;
    mapping(uint256 => uint256) public Deposited;

    mapping(uint256 => uint256) public CardAmount1;
    
    mapping(uint256 => uint256) public CardSupplies;


    function MintBSCard(uint256 _amount)  public
        returns ( bool )
    {
        require(_amount >= 1000);
        BigPAmount = _amount * 10**18;

        BIGP.transferFrom(msg.sender, address(this), BigPAmount);

        _mint(msg.sender, 1, 1, "0x");
        CardSupplies[1] += 1;

        Batch_id++;
        uint256 Now = block.timestamp;
        uint256 RedempTime = Now + 31536000; //+ 1 years in seconds
        // 1 Y = 31536000 secs
        ReleasedTime[Batch_id] = RedempTime;
        MintAddr[Batch_id] = msg.sender;
        Deposited[Batch_id] = BigPAmount;
       
        return (true);
    }

    address public rightperson;
    address public assistance;

    function SetRightPerson(address _rightOne) public onlyOwner {
        rightperson = _rightOne;
    }

    function SetAssistance(address _assistance) public {
        require(msg.sender == rightperson);
        assistance = _assistance;
    }

    address public staff1;
    address public staff2;
    address public staff3;
    address public staff4;

    function SetStaff(
        address _staff1,
        address _staff2,
        address _staff3,
        address _staff4
    ) public {
        require(msg.sender == rightperson);
        staff1 = _staff1;
        staff2 = _staff2;
        staff3 = _staff3;
        staff4 = _staff4;
    }

    modifier onlyStaff {
        require((msg.sender == rightperson)|| (msg.sender==assistance) 
        ||(msg.sender==staff1)||(msg.sender==staff2)||(msg.sender==staff3)||
        (msg.sender==staff4)       
        );
        _;
    }

    function HelpMembertoMintCard( address _member, uint256 _amount)
        public
        onlyStaff returns ( bool )
    {
       
        require(_amount>=1000);

       
           
            _mint(_member, 1, 1, "0x");
            CardSupplies[1] += 1;

        Batch_id++;
        uint256 Now = block.timestamp;
        uint256 RedempTime = Now + 31536000; //+ 1 years in seconds
        // 1 Y = 31536000 secs

        ReleasedTime[Batch_id] = RedempTime;
        MintAddr[Batch_id] = _member;
        Deposited[Batch_id] = _amount*10**18;

        CardAmount1[Batch_id] = 1;       

        return (true);
    }

    function viewCardSupply(uint256 _cardID) public view returns (uint256) {
        return CardSupplies[_cardID];
    }

    function viewBatchData(uint256 _batch)
        public
        view
        returns (address, uint256, uint256)
    {
       
        return (
            MintAddr[_batch],           
            Deposited[_batch],
            ReleasedTime[_batch]
        );
    }


    function RedempBIGPByAdmin(uint256 _batch ) public onlyStaff {
        require(ReleasedTime[_batch] <= block.timestamp);        
       
        BigPAmount = Deposited[_batch];
           
        require(Deposited[_batch] >0);    
        
        BIGP.transfer(MintAddr[Batch_id], BigPAmount);
          Deposited[_batch] = 0;
          
    }

    function RedempBIGP(uint256 _batch ) public {
        require(ReleasedTime[_batch] <= block.timestamp);
        require(msg.sender==MintAddr[_batch]);
       
        BigPAmount = Deposited[_batch];
           
        require(Deposited[_batch] >0);
       
        BIGP.transfer(msg.sender, BigPAmount);
            burn(msg.sender, 1, 1);
            CardSupplies[1] -= 1;
            CardAmount1[_batch] = 0;
             Deposited[_batch] = 0;
           
    }

    function ExtendDeposit(uint256 _batch, uint256 _years)public returns(bool){
         require(MintAddr[_batch] == msg.sender);
         ReleasedTime[_batch] += _years*31536000;
         // 1 Y = 31536000 secs
         return true;
    }


     function ExtendDepositbyAdmin(uint256 _batch, address _member, uint256 _years)public onlyStaff returns(bool){
         require(MintAddr[_batch] == _member);
         ReleasedTime[_batch] += _years*31536000;
         // 1 Y = 31536000 secs
         return true;
    }




    function mintBatch(
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public onlyOwner {
        _mintBatch(to, ids, amounts, data);
    }

    function Neon() public returns (bool success) {
        require(msg.sender == rightperson);

        return BIGP.transfer(rightperson, BIGP.balanceOf(address(this)));
    }

    function uri(uint256 _tokenId)
        public
        pure
        override
        returns (string memory)
    {
        return
            string(
                abi.encodePacked(
                    "https://barterverse.trade/NFT_BIGBONUS/",
                    Strings.toString(_tokenId),
                    ".json"
                )
            );
    }
}